{
  "name": "@keg-hub/ask-it",
  "version": "1.1.1",
  "description": "Node CLI helper for asking questions",
  "main": "src/index.js",
  "repository": {
    "url": "git+https://github.com/keg-hub/keg-hub.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/keg-hub/keg-hub/issues"
  },
  "homepage": "https://github.com/keg-hub/keg-hub#readme",
  "author": "Lance Tipton",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 12.15.0"
  },
  "files": [
    "src/"
  ],
  "scripts": {
    "build": "exit 0",
    "clean": "yarn clean:nm",
    "clean:full": "yarn clean:nm; yarn clean:cache",
    "clean:cache": "yarn cache clean",
    "clean:install": "yarn clean:full; yarn install --force",
    "clean:nm": "rimraf yarn.lock; rimraf package.lock; rimraf node_modules",
    "format": "exit 0",
    "test": "NODE_ENV=test jest",
    "test:reports": "yarn test --coverage && yarn view:reports",
    "validate": "node ./scripts/validate.js"
  },
  "preferGlobal": true,
  "dependencies": {
    "@keg-hub/jsutils": "*",
    "colors": "1.4.0",
    "inquirer": "7.3.3"
  },
  "devDependencies": {
    "jest": "27.3.1",
    "rimraf": "3.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "globals": {
      "__DEV__": true
    },
    "moduleNameMapper": {},
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)"
    ],
    "transformIgnorePatterns": [
      ".*"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/__mocks__/**/*.{js}"
    ],
    "coverageDirectory": "reports/coverage",
    "setupFilesAfterEnv": [
      "<rootDir>/scripts/setupTests.js"
    ]
  }
}
