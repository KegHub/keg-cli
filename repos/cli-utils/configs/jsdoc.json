{
  "tags": {
    "allowUnknownTags": true
  },
  "markdown": {
    "hardwrap": true
  },
  "source": {
    "include": [
      "README.md",
      "src/"
    ],
    "excludePattern": "(__mocks__|__tests__|node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "../../node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "tutorials": "src/tutorials",
    "recurse": true,
    "verbose": true
  },
  "docdash": {
    "static": true,
    "sort": true,
    "sectionOrder": [
      "Tutorials",
      "Modules",
      "Classes"
    ],
    "disqus": "",
    "openGraph": {
      "title": "CLIJS Utils",
      "type": "website",
      "site_name": "cli-utils",
      "url": "https://github.com/keg-hub/keg-cli/tree/develop/repos/cli-utils"
    },
    "meta": {
      "title": "CLI Utils",
      "description": "Utility methods for building Node.js CLI's",
      "keyword": "utils, cli, task, tasks"
    },
    "commonNav": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "private": false,
    "removeQuotes": "none",
    "scripts": [
      "styles/docs.css",
      "scripts/docs.js"
    ],
    "menu": {
      "Github repo": {
        "href":"https://github.com/keg-hub/keg-cli/tree/develop/repos/cli-utils",
        "target":"_blank",
        "class":"menu-item",
        "id":"repository"
      }
    }
  }
}