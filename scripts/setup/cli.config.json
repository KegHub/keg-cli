{
  "name": "keg-cli",
  "displayName": "Keg CLI",
  "docker": {
    "providerUrl": "ghcr.io",
    "namespace": "keghub",
    "user": "",
    "token": ""
  },
  "cli": {
    "paths": {
    },
    "git": {
      "orgName": "keghub",
      "orgUrl": "https://github.com/keghub",
      "provider": "https://github.com",
      "publicToken": "",
      "repos": {
        "hub": "keg-hub",
        "cli": "keg-cli"
      }
    },
    "taps": {},
    "settings": {
      "defaultEnv": "local",
      "defaultDomain": "keghub.io",
      "docker": {
        "force": true,
        "buildX": true,
        "buildKit": true,
        "preConfirm": true,
        "defaultLocalBuild": true,
        "imagePullPolicy": "Always",
        "defaultTag": "develop"
      },
      "checkUpdates": false,
      "editorCmd": "code",
      "errorStack": false,
      "git": {
        "secure": false
      },
      "task": {
        "optionsAsk": true
      }
    }
  },
  "publish": {
    "default": {
      "tasks": {
        "install": true,
        "test": true,
        "build": true,
        "publish": true,
        "commit": true
      }
    },
    "spawn-cmd": {
      "name": "spawn-cmd",
      "dependent": false,
      "order": {
        "0": "@keg-hub/spawn-cmd"
      }
    },
    "ask-it": {
      "name": "ask-it",
      "dependent": false,
      "order": {
        "0": "@keg-hub/ask-it"
      }
    },
    "args-parse": {
      "name": "args-parse",
      "dependent": false,
      "order": {
        "0": "@keg-hub/args-parse"
      }
    }
  }
}
